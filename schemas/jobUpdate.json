{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.job.com/jobUpdate.schema.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Secretary"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "minimum": 0,
            "title": "The salary Schema",
            "examples": [
                100
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "minimum": 0,
            "maximum": 1.0,
            "title": "The equity Schema",
            "examples": [
                0.1
            ]
        }
    },
    "additionalProperties": false,
    "required": []
  }
  